{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Application": {
    "Clustering": {
      "Provider": "Localhost"
    },
    "Persistence": {
      "Provider": "DynamoDB",
      "DynamoDB": {
        "TableName": "seamless-world-server-db",
        "ServiceId": "seamless-world",
        "Service": "http://localhost:8000"
      }
    },
    "GrainDirectory": {
      "Provider": "Memory",
      "Redis": {
        "ConnectionString": "localhost:6379"
      }
    }
  }
}